<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView_HS_imgFaceOff"
        android:layout_width="183dp"
        android:layout_height="82dp"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="16dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="61dp"
        android:layout_height="76dp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="190dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView_HS_washedHands"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="You have washed your hands 0 times today."
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="216dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="62dp"
        android:layout_height="63dp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="114dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView_HS_touchedFace"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="You have touched your face 0 times today."
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="137dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="58dp"
        android:layout_height="57dp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="289dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView_HS_leftHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="You have left your home 0 times today."
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="310dp" />
</androidx.constraintlayout.widget.ConstraintLayout>